{
	"name": "dface/tito",
	"description": "This is a small class that helps you to construct a lightweight command line tool for your application",
	"keywords": ["CLI", "shell", "console", "tool", "container", "service", "call"],
	"type": "library",
	"license": "MIT",
	"authors": [{
		"name": "Denis Ponomarev",
		"email": "ponomarev@gmail.com"
	}
	],
	"require": {
		"php": ">=7.4",
	  	"symfony/yaml": "^5.1",
		"ext-json": "*",
		"ext-mbstring": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.3.5"
	},
	"suggest": {
		"ext-mbstring": "MBstring or iconv required to work with non-utf encodings"
	},
	"autoload": {
		"psr-0": {"dface\\tito\\": "src/"}
	},
	"config": {
		"platform": {
			"php": "8.2.5"
		}
	}
}
